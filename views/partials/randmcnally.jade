block content
  .container-fluid(ng-cloak='')
    .row.top
      .row-fluid.naver
        .col-xs-4
          h4.naver.l Lendeavor
          a(href='/lendeavor')
            img.naver.l(src='/images/leftc.png')          
        .col-xs-4.center         
        .col-xs-4.right
          h4.naver stockickr
