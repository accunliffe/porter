@import url('http://fonts.googleapis.com/css?family=Raleway:400,800,700')
@import url('http://fonts.googleapis.com/css?family=Lato:400,700,900,400italic,700italic,900italic')
@font-face 
  font-family:'Arca'
  src url('/fonts/Arca-Heavy.ttf')

[ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak
  display none !important  

body
  padding 0 0 0 0 !important

//main containers


.container-fluid
  padding 0 0 0 0 !important
  &.content
    height auto
    padding 0 0 0 0 !important


//navigation

nav  
  &.navbar
    display block
    background-color rgba(215,215,215,0.95)
    max-height 50px
    @media screen and (min-width: 480px)
      min-height 50px !important
  &.mobile
    @media screen and (min-width: 900px)
      display none
    @media screen and (max-height 480px) and (orientation: landscape)
      display block    
  &.desktop
    @media screen and (max-width: 900px)
      display none
    @media screen and (max-height 480px) and (orientation: landscape)
      display none
  p.navbar-text    
    &.naver
      cursor pointer
      @media screen and (max-width: 800px)
        text-align right
        display inline-block
        font-size 10px
        width 20px
      margin-right 10px 
      margin-left 10px
      letter-spacing 2px
      font-family:'Raleway'
      -webkit-font-smoothing antialiased
      color rgba(115, 115, 115, 1)
      -webkit-transition color, text-decoration, cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.5s
      -moz-transition color, text-decoration, cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.5s
      -o-transition color, text-decoration, cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.5s
      transition color, text-decoration, cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.5s
    &.naver:hover
      color black !important
      text-decoration underline
  
ul
  &.dropdown-menu
    background-color rgba(215,215,215,0.95)
    color black
      
.dropdown-menu
  & li
    & a
      -webkit-font-smoothing antialiased
      color rgba(115, 115, 115, 1)
      font-family: 'Raleway'
      text-align center
    a:hover
      color black !important

  p  
    &.naver
      cursor pointer
      @media screen and (max-width: 800px)
        text-align right
        display inline-block
        font-size 10px
        width 20px
      margin-right 10px 
      margin-left 10px
      letter-spacing 2px
      font-family:'Raleway'
      color white !important

//shifty icon
.shift-on-the-icon
  -webkit-transition background-image cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.5s
  -moz-transition background-image cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.5s
  -o-transition background-image cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.5s
  transition background-image cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.5s

.icon
  display block
  margin 2.5px 0 0 5px
  width 45px
  height 45px
  max-width 45px
  max-height 45px
  vertical-align center
  position fixed
  background-image url('data:image/png;base64,')
  background-repeat no-repeat
  &:hover
    cursor pointer
    background-image url("data:image/png;base64,")
.iconM
  display block
  margin 2.5px 0 0 5px
  width 45px
  height 45px
  max-width 45px
  max-height 45px
  vertical-align center
  position fixed
  background-image url('data:image/png;base64,')
  background-repeat no-repeat  

